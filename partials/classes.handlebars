
<div class="page-header">
	<h4>
		{{#cleanDisplayName name}}
			<span class="label label-default">namespace</span> {{namespace}}
		{{/cleanDisplayName}}
	</h4>
	<h4>
		{{#cleanDisplayName name}}
			<span class="label label-default">class</span> {{displayName}}
		{{/cleanDisplayName}}
	</h4>
</div>
{{#if deprecated}}
	<div class="bs-callout bs-callout-warning">
		<h4>Deprecated</h4>
		<p>
			{{#if deprecationMessage}}
				{{deprecationMessage}}
			{{else}}
				This class is deprecated.
			{{/if}}
		</p>
	</div>
{{/if}}

<div class="bs-callout bs-callout-info">

	{{#if uses}}
		<div class="uses">
			Uses
			<ul class="inline commas">
				{{#uses}}
					<li><a href="{{.}}.html">{{.}}</a></li>
				{{/uses}}
			</ul>
		</div>
	{{/if}}

	{{#if extends}}
		<div class="extends">
			Extends: <code>{{#crossLink extends}}{{/crossLink}}</code>
		</div>
	{{/if}}

	{{#if foundAt}}
		<div class="foundat">
			Defined in: <a href="{{foundAt}}">`{{{file}}}:{{{line}}}`</a>
		</div>
	{{/if}}

	{{#if module}}
		<p>
		{{#if submodule}}
			Module: {{#crossLinkModule submodule}}{{/crossLinkModule}}<br>
			Parent Module: {{#crossLinkModule module}}{{/crossLinkModule}}
		{{else}}
			Module: {{#crossLinkModule module}}{{/crossLinkModule}}
		{{/if}}
		</p>
	{{/if}}

	{{#if since}}
		<p>Available since {{since}}</p>
	{{/if}}

	{{#if classDescription}}
		<h4>Description: </h4>
	{{else}}
		{{#if documentation}}
			<h4>Description: </h4>
		{{/if}}
	{{/if}}

	{{#if classDescription}}
		{{{classDescription}}}
	{{/if}}

	{{#if documentation}}
		{{#parseDocumentation documentation}}{{/parseDocumentation}}
	{{/if}}
</div>

{{#is_constructor}}
	{{>method}}
{{/is_constructor}}

<div>
	<ul class="nav nav-tabs">
		<li class="active">
			<a data-toggle="tab" href="#index">Index</a>
		</li>

		{{#if methods}}
			<li><a data-toggle="tab" href="#methods">Methods</a></li>
		{{/if}}

		{{#if properties}}
			<li><a data-toggle="tab" href="#properties">Properties</a></li>
		{{/if}}

		{{#if attrs}}
			<li><a data-toggle="tab" href="#attrs">Attributes</a></li>
		{{/if}}

		{{#if events}}
			<li><a data-toggle="tab" href="#events">Events</a></li>
		{{/if}}
	</ul>

	<div class="tab-content">
		<div id="index" class="tab-pane active">

			<h2 class="off-left">Index</h2>

			{{#if methods}}
				<div class="panel panel-default">
					<div class="panel-heading">Methods</div>

					<table class="table">
						<thead>
						<tr>
							<th>Scope:</th>
							<th>Name:</th>

							<th>Description:</th>
						</tr>
						</thead>
						<tbody>
						{{#methods}}
							<tr class="index-item method{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}{{#if extended_from}} inherited{{/if}}">
								<td>
									<span class="label label-primary static">{{#if access}} {{access}}{{else}}public{{/if}}</span>


									{{#if static}}
										<span class="label label-primary static">static</span>
									{{/if}}

									{{#if deprecated}}
										<span class="label label-primary deprecated">deprecated</span>
									{{/if}}
								</td>
								<td><a href="#" data-tabid="#methods" data-anchor="#method_{{name}}"
								       data-tablink="{{name}}">{{name}}</a></td>
								<td>
								<td></td>
							</tr>
						{{/methods}}
						</tbody>
					</table>
				</div>
			{{/if}}

			{{#if properties}}

				<div class="panel panel-default">
					<div class="panel-heading">Properties</div>

					<table class="table">
						<thead>
						<tr>
							<th>Scope:</th>
							<th>Name:</th>

							<th>Description:</th>
						</tr>
						</thead>
						<tbody>
						{{#properties}}
							<tr class="index-item method{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}{{#if extended_from}} inherited{{/if}}">
								<td>
									<span class="label label-primary static">{{#if access}} {{access}}{{else}}public{{/if}}</span>


									{{#if static}}
										<span class="label label-primary static">static</span>
									{{/if}}

									{{#if deprecated}}
										<span class="label label-primary deprecated">deprecated</span>
									{{/if}}
								</td>
								<td><a href="#" data-tabid="#methods" data-anchor="#method_{{name}}"
								       data-tablink="{{name}}">{{name}}</a></td>
								<td>
								<td></td>
							</tr>
						{{/properties}}
						</tbody>
					</table>
				</div>
			{{/if}}

			{{#if attrs}}

				<div class="panel panel-default">
					<div class="panel-heading">Attributes</div>

					<table class="table">
						<thead>
						<tr>
							<th>Scope:</th>
							<th>Name:</th>

							<th>Description:</th>
						</tr>
						</thead>
						<tbody>
						{{#attrs}}
							<tr class="index-item method{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}{{#if extended_from}} inherited{{/if}}">
								<td>

									<span class="label label-primary static">{{#if access}} {{access}}{{else}}public{{/if}}</span>

									{{#if static}}
										<span class="label label-primary static">static</span>
									{{/if}}

									{{#if deprecated}}
										<span class="label label-primary deprecated">deprecated</span>
									{{/if}}
								</td>
								<td><a href="#"  data-tabid="#attrs" data-anchor="#attr_{{name}}"
								       data-tablink="{{name}}">{{name}}</a></td>
								<td>
								<td></td>
							</tr>
						{{/attrs}}
						</tbody>
					</table>
				</div>

			<!--
				<div class="index-section attrs">
					<h3>Attributes</h3>

					<ul class="index-list attrs{{#if extends}} extends{{/if}}">
						{{#attrs}}
							<li class="index-item attr{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}{{#if extended_from}} inherited{{/if}}">
								<a href="#" data-tabid="#attrs" data-anchor="#attr_{{name}}"
								   data-tablink="{{name}}">{{name}}</a>
							</li>
						{{/attrs}}
					</ul>
				</div>
				-->
			{{/if}}

			{{#if events}}

				<div class="panel panel-default">
					<div class="panel-heading">Attributes</div>

					<table class="table">
						<thead>
						<tr>
							<th>Scope:</th>
							<th>Name:</th>

							<th>Description:</th>
						</tr>
						</thead>
						<tbody>
						{{#events}}
							<tr class="index-item method{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}{{#if extended_from}} inherited{{/if}}">
								<td>

									<span class="label label-primary static">{{#if access}} {{access}}{{else}}public{{/if}}</span>

									{{#if static}}
										<span class="label label-primary static">static</span>
									{{/if}}

									{{#if deprecated}}
										<span class="label label-primary deprecated">deprecated</span>
									{{/if}}
								</td>
								<td><a href="#" data-tabid="#methods" data-anchor="#method_{{name}}"
								       data-tablink="{{name}}">{{name}}</a></td>
								<td>
								<td></td>
							</tr>
						{{/events}}
						</tbody>
					</table>
				</div>
				<!--
				<div class="index-section events">
					<h3>Events</h3>

					<ul class="index-list events{{#if extends}} extends{{/if}}">
						{{#events}}
							<li class="index-item event{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}{{#if extended_from}} inherited{{/if}}">
								<a href="#" data-tabid="#events" data-anchor="#event_{{name}}"
								   data-tablink="{{name}}">{{name}}</a>

								{{#if static}}
									<span class="flag static">static</span>
								{{/if}}
								{{#if deprecated}}
									<span class="flag deprecated">deprecated</span>
								{{/if}}
							</li>
						{{/events}}
					</ul>
				</div>
				-->
			{{/if}}
		</div>

		{{#if methods}}
			<div id="methods" class="tab-pane">
				<h2 class="off-left">Methods</h2>

				{{#methods}}
					{{>method}}
				{{/methods}}
			</div>
		{{/if}}

		{{#if properties}}
			<div id="properties" class="tab-pane">
				<h2 class="off-left">Properties</h2>

				{{#properties}}
					{{>props}}
				{{/properties}}
			</div>
		{{/if}}

		{{#if attrs}}
			<div id="attrs" class="tab-pane">
				<h2 class="off-left">Attributes</h2>
{{#attrs}}{{>attrs}}{{/attrs}}
			</div>
		{{/if}}

		{{#if events}}
			<div id="events" class="tab-pane">
				<h2 class="off-left">Events</h2>

				{{#events}}
					{{>events}}
				{{/events}}
			</div>
		{{/if}}
	</div>
</div>
